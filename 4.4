//Побитовая стрелка Пирса предпоследней цифры всех чисел (операции выполняются
//справа налево)

fun main() = println(
    readlnOrNull()
        ?.takeIf { it.isNotEmpty() }
        ?.split(" ")
        ?.mapNotNull { it.toIntOrNull() }
        ?.mapNotNull { n ->
            n.toString().let { s ->
                if (s.length >= 2) s[s.length - 2].digitToInt() else null
            }
        }
        ?.takeIf { it.isNotEmpty() }
        ?.reduceRight { a, b -> (a or b).inv() }
        ?: "Нет цифр"
)
